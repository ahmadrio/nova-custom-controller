{
    "name": "opanegro/nova-custom-controller",
    "description": "Make custom controller in Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "custom",
        "controller"
    ],
    "authors": [
        {
            "name": "Ahmad Rio",
            "email": "ahmadrio28asia@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Opanegro\\NovaCustomController\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Opanegro\\NovaCustomController\\NovaCustomControllerProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
