{
    "name": "pt-dot-playground/nova-custom-controllers",
    "description": "Make custom controller in Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "Ahmad Rio",
            "email": "ahmadrio28asia@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "PtDotPlayground\\NovaCustomControllers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PtDotPlayground\\NovaCustomControllers\\NovaCustomControllersProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
